<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item android:id="@+id/background">
        <shape android:shape="rectangle">
            <solid android:color="#ff000000" />
            <corners
                android:radius="3dp"   >
            </corners>
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="2dp"
                android:color="@android:color/darker_gray" >
            </stroke>
            <corners
                android:radius="2dp"   >
            </corners>
        </shape>
    </item>

</layer-list>