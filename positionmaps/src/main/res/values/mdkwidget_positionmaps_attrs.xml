<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MDKCommons.MDKMapsPositionComponent">

        <attr name="addressTextOnMapViewId" format="reference" />
        <attr name="locationTextOnMapViewId" format="reference" />

        <attr name="mapsMode" format="enum">
            <enum name="geopoint" value="0"/>
            <enum name="gps" value="1"/>
            <enum name="places_picker" value="2"/>
        </attr>

        <attr name="displayMode" format="integer">
            <flag name="coord" value="0x000000ff"/>
            <flag name="address" value="0x0000ff00"/>
            <flag name="markerCoord" value="0x00ff0000"/>
            <flag name="markerAddress" value="0xff000000"/>
        </attr>

        <attr name="zoom" format="integer"/>

        <attr name="gpsMarker" format="reference"/>

        <attr name="addressMarker" format="reference"/>

    </declare-styleable>

</resources>